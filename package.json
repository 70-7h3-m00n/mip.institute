{
  "name": "mip",
  "version": "1.0.0",
  "description": "МИП — Станьте психологом с нуля или освойте новые навыки дистанционно из своего города",
  "main": "index.html",
  "scripts": {
    "start": "node app.js",
    "dev": "rm -rf ./development && rm -rf ./.cache && parcel src/index.html --out-dir development -p 3000",
    "build": "parcel build src/index.html --out-dir dist --public-url ./ && npx postcss dist/*.css --no-map --use autoprefixer -d dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Novailoveyou/psycho-site.git"
  },
  "author": "Novailoveyou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Novailoveyou/psycho-site/issues"
  },
  "homepage": "https://github.com/Novailoveyou/psycho-site#readme",
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/runtime-corejs2": "^7.12.13",
    "autoprefixer": "^10.2.4",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^4.0.0",
    "sass": "^1.32.7",
    "postcss-cli": "^8.3.1"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
