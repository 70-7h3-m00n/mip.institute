{
  "name": "mip",
  "version": "1.0.0",
  "description": "МИП — Станьте психологом с нуля или освойте новые навыки дистанционно из своего города",
  "main": "index.html",
  "homepage": ".",
  "scripts": {
    "sass": "node-sass -w src/scss/ -o src/css/ --recursive",
    "deploy": "gh-pages -d dist",
    "start": "node app.js",
    "dev": "rm -rf ./development && rm -rf ./.cache && parcel src/index.html --out-dir development -p 3000",
    "build": "parcel build src/index.html --out-dir dist --public-url ./"
  },
  "author": "Novailoveyou",
  "license": "MIT",
  "dependencies": {
    "@glidejs/glide": "^3.4.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gh-pages": "^2.2.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.6"
  },
  "engines": {
    "node": "14.x",
    "npm": "7.x"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/runtime-corejs2": "^7.11.2",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.27.0"
  }
}
