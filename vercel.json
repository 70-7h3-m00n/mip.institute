{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/api/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=3600, stale-while-revalidate=360"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "mip.institute, mip-institute-git-dev-ipe.vercel.app"
        }
      ]
    }
  ]
}
